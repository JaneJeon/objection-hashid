    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "knex": "^0.20.0",
    "lint-staged": "^10.0.2",
    "objection-1": "npm:objection@1",
    "objection-2": "npm:objection@2",
    "objection-visibility": "^0.4.0",
    "prettier": "^1.18.2",
    "snazzy": "^8.0.0",
    "sqlite3": "^4.1.0",
    "standard": "^14.1.0"
  },
