  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --ci --reporters=default --reporters=jest-junit",
    "lint": "standard | snazzy && prettier --check '**/*.{md,json,yml,yaml}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
