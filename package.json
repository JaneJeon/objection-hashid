    "husky": "^3.0.5",
    "is-ci": "^2.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "knex": "^0.20.0",
    "lint-staged": "^9.2.5",
    "objection": "^1.6.9",
    "objection-visibility": "^0.4.0",
    "prettier": "^1.18.2",
    "snazzy": "^8.0.0",
    "sqlite3": "^4.1.0",
    "standard": "^14.1.0"
  },
  "keywords": [
